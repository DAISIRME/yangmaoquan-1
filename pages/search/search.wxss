/* pages/search/search.wxss */
@import "/wxSearch/wxSearch.wxss";
.reasult {
  width: 90%;
  height: 200rpx;
  margin-top: 20rpx;
  display: flex;
}
.img {
  width: 200rpx;
  height: 200rpx;
}
.search-img {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
  margin-top: 18rpx;
}
.msg {
  width: 65%;
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content: space-between;
